{
  "cSpell.userWords": [
    "Baike",
    "Bizline",
    "Byted",
    "Gulu",
    "Kani",
    "Kiko",
    "Millis",
    "Qrcode",
    "Querys",
    "SSMLTAG",
    "Shougo",
    "Sider",
    "Trangle",
    "Uids",
    "actix",
    "ailab",
    "antd",
    "bindgen",
    "bytedance",
    "bytedesign",
    "ctrlp",
    "ctrlpvim",
    "devtools",
    "echodoc",
    "litbot",
    "mhartington",
    "renderable",
    "ssml",
    "struct",
    "ttml",
    "ttss",
    "vsplit",
    "vuetify",
    "wasm",
    "weixin"
  ],
  "coc-webpack.enable": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.formatOnSaveFiletypes": [
    "java",
    "javascript",
    "typescript",
    "html",
    "css",
    "python",
    "typescript.tsx",
	"markdown"
  ],
  "coc.preferences.formatOnType": true,
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.snippetStatusText": "Ⓢ ",
  "codeLens.enable": true,
  "diagnostic.displayByAle": false,
  "diagostic.errorSign": "﮻",
  "diagnostic.infoSign": "",
  "diagnostic.signOffset": 9999999,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": " ❯❯❯ ",
  "diagnostic.warningSign": "",
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "typescript.tsx",
    "vue"
  ],
  "git.addGBlameToBufferVar": false,
  "git.addGBlameToVirtualText": false,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.addedSign.text": "█",
  "git.branchCharacter": "",
  "git.changeRemovedSign.hlGroup": "GitGutterChange",
  "git.changeRemovedSign.text": "█",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.changedSign.text": "█",
  "git.enableGlobalStatus": false,
  "git.enableGutters": true,
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.removedSign.text": "▁",
  "git.signOffset": 2,
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.text": "▔",
  "git.virtualTextPrefix": " ❯❯❯ ",
  "html.format.enable": true,
  "javascript.referencesCodeLens.enable": true,
  "javascript.showUnused": true,
  "javascript.suggest.names": true,
  "javascript.suggestionActions.enabled": true,
  "json.format.enable": true,
  "languageserver": {
    "ttml": {
      "args": ["--node-ipc"],
      "module": "~/code/ttml-lsp/workers/language-server.js",
      "filetypes": ["ttml"]
    }
  },
  "npm.binPath": "yarn",
  "prettier.eslintIntegration": true,
  "python.autoComplete.addBrackets": true,
  "python.formatting.provider": "yapf",
  "python.jediEnabled": false,
  "signature.enable": true,
  "signature.target": "float",
  "suggest.completionItemKindLabels": {
    "class": "",
    "color": "",
    "constant": "",
    "default": "",
    "enum": "",
    "enumMember": "",
    "event": "鬒",
    "field": "綠",
    "file": "",
    "folder": "",
    "function": "ƒ",
    "interface": "",
    "keyword": "",
    "method": "",
    "module": "",
    "operator": "洛",
    "property": "",
    "reference": "渚",
    "snippet": "",
    "struct": "פּ",
    "text": "",
    "typeParameter": "",
    "unit": "",
    "value": "",
    "variable": ""
  },
  "suggest.detailField": "preview",
  "suggest.detailMaxLength": 60,
  "suggest.echodocSupport": true,
  "suggest.enablePreselect": false,
  "suggest.enablePreview": true,
  "suggest.floatEnable": true,
  "suggest.noselect": true,
  "suggest.snippetIndicator": "",
  "suggest.timeout": 5000,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.triggerCompletionWait": 100,
  "typescript.referencesCodeLens.enable": false
}
